{
    "name": "nextjs-app-skeleton",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "generate:queries": "orval --config orval.config.js",
        "format": "prettier --write .",
        "cypress": "cypress open",
        "cypress:headless": "cypress run",
        "test:e2e": "start-server-and-test start http://localhost:3000 cypress",
        "test:e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
        "typecheck": "tsc --noEmit --project tsconfig.json"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@fontsource/blinker": "^4.5.7",
        "@fontsource/roboto": "^4.5.7",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.8.6",
        "axios": "^0.27.2",
        "next": "12.1.6",
        "next-auth": "^4.10.0",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-query": "^3.39.1"
    },
    "devDependencies": {
        "@testing-library/cypress": "^8.0.3",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@types/node": "17.0.40",
        "@types/react": "18.0.12",
        "@types/react-dom": "18.0.5",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.18.0",
        "eslint-config-kentcdodds": "^20.3.1",
        "eslint-config-next": "12.1.6",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "orval": "^6.8.1",
        "prettier": "2.7.1",
        "typescript": "4.7.3",
        "typescript-styled-plugin": "^0.18.2"
    },
    "lint-staged": {
        "*.{css,less,scss,html,json,jsx,js}": [
            "prettier --write ."
        ],
        "*.js": "eslint --fix"
    },
    "eslintIgnore": [
        "node_modules",
        "coverage",
        "dist",
        "src/common/queries"
    ]
}
